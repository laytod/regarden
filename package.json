{
  "name": "regarden-nonprofit",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "clean": "rm -rf .next out",
    "dev": "npm run clean && next dev",
    "dev:fast": "next dev",
    "build": "node scripts/fetch-calendar-to-json.mjs && npm run clean && next build",
    "serve": "npx serve out",
    "serve:nginx": "docker run --rm -v \"$(pwd)/out:/usr/share/nginx/html:ro\" -p 8080:80 nginx:alpine",
    "serve:ngrok": "npx ngrok http 8080",
    "lint": "next lint",
    "test": "jest --forceExit",
    "test:watch": "jest --watch --forceExit",
    "test:coverage": "jest --coverage --forceExit",
    "calendar:test": "node scripts/test-calendar-fetch.mjs"
  },
  "dependencies": {
    "@fullcalendar/daygrid": "^6.1.10",
    "@fullcalendar/interaction": "^6.1.10",
    "@fullcalendar/react": "^6.1.10",
    "@fullcalendar/timegrid": "^6.1.10",
    "@hookform/resolvers": "^5.2.2",
    "date-fns": "^2.30.0",
    "ical-expander": "^3.2.0",
    "next": "^14.0.0",
    "node-ical": "^0.25.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "canvas": "^3.2.1",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "pdf-image-extractor": "^2.0.8",
    "pdf-parse": "^2.4.5",
    "pdf-poppler": "^0.2.3",
    "pdfjs-dist": "^5.4.530",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.0.0"
  }
}
